<Window x:Class="FMartFUDAApp.FinancialManagement"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FMartFUDAApp"
        mc:Ignorable="d"
        Title="FinancialManagement" WindowState="Maximized" Background="#F5F5F5">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Revenue" Click="Revenue"/>
            <MenuItem Header="ManagerStaff" Click="ManagerStaff"/>
            <MenuItem Header="FinacialReport" Click="FinacialReport"/>

        </Menu>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>

    </DockPanel>
</Window>
